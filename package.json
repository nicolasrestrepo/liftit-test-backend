{
  "name": "backend-liftit",
  "version": "1.0.0",
  "description": "simple server for test liftit, using expres.js ",
  "main": "server.js",
  "scripts": {
    "flow": "flow",
    "flow:check": "flow check ./",
    "flow:build": "flow-remove-types ./ -d ./lib/ --all --pretty",
    "flow:deps": "flow-typed install",
    "lint": "eslint ./",
    "dev": "nodemon server.js",
    "start": "NODE_ENV=production node server.js"
